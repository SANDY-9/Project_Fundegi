<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.my.MY_">

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="마이페이지"
        android:textColor="@color/fundeagi_first1"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline7"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="56dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:background="?android:attr/selectableItemBackground"
        android:onClick="goBack"
        android:scaleType="center"
        app:layout_constraintBottom_toTopOf="@+id/guideline7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_round_arrow_back"
        app:tint="@color/fundeagi_first1" />

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_basic1"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="@dimen/margin_basic1"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline7">

        <androidx.cardview.widget.CardView
            android:layout_width="@dimen/profile_size"
            android:layout_height="@dimen/profile_size"
            app:cardCornerRadius="5dp"
            app:cardElevation="0dp"
            app:cardMaxElevation="0dp">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fundegi_icon" />
        </androidx.cardview.widget.CardView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/margin_basic1"
            android:orientation="vertical"
            android:padding="5dp">

            <TextView
                android:id="@+id/textView33"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:letterSpacing="0.1"
                android:text="펀데기님"
                android:textSize="25dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="펀데기 은행 123-23456-76532"
                android:textColor="@color/register2"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/textView35"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginStart="5dp"
                android:gravity="bottom"
                android:text="sandy@gmail.com"
                android:textColor="@color/register1"
                android:textSize="14dp" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view13"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="20dp"
        android:background="@color/register2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7" />

    <View
        android:id="@+id/view14"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="20dp"
        android:background="@color/register2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8" />

    <Button
        android:id="@+id/button_logout"
        style="@style/Widget.MaterialComponents.Button.TextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="10dp"
        android:letterSpacing="0.03"
        android:text="@string/menu_title5"
        android:textColor="@color/register1"
        android:textSize="15dp"
        android:textStyle="bold"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:rippleColor="#D6D6D6" />

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="0dp"
        android:layout_height="350dp"
        android:layout_marginStart="@dimen/margin_basic1"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="@dimen/margin_basic1"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view13">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button_menu1"
                style="@style/Theme.Project_Fundegi"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1"
                android:background="@drawable/round"
                android:elevation="3dp"
                android:gravity="top"
                android:letterSpacing="-0.03"
                android:paddingStart="@dimen/margin_basic1"
                android:paddingTop="@dimen/margin_basic1"
                android:text="@string/menu_title1"
                android:textSize="25dp"
                android:textStyle="bold"
                android:onClick="onClick"
                app:backgroundTint="@color/fundeagi_second1"
                app:rippleColor="#D6D6D6" />

            <Button
                android:id="@+id/button_menu2"
                style="@style/Theme.Project_Fundegi"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1"
                android:background="@drawable/round"
                android:elevation="3dp"
                android:gravity="top"
                android:letterSpacing="-0.03"
                android:paddingStart="@dimen/margin_basic1"
                android:paddingTop="@dimen/margin_basic1"
                android:text="@string/menu_title2"
                android:textColor="@color/white"
                android:textSize="25dp"
                android:textStyle="bold"
                android:onClick="onClick"
                app:backgroundTint="@color/fundeagi_first1"
                app:rippleColor="#D6D6D6" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button_menu3"
                style="@style/Theme.Project_Fundegi"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_weight="1"
                android:background="@drawable/round"
                android:elevation="3dp"
                android:gravity="top"
                android:letterSpacing="-0.03"
                android:onClick="onClick"
                android:paddingStart="@dimen/margin_basic1"
                android:paddingTop="@dimen/margin_basic1"
                android:text="@string/menu_title3"
                android:textColor="@color/white"
                android:textSize="25dp"
                android:textStyle="bold"
                app:backgroundTint="@color/fundeagi_second2"
                app:rippleColor="#D6D6D6" />

            <Button
                android:id="@+id/button_menu4"
                style="@style/Theme.Project_Fundegi"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:background="@drawable/round"
                android:elevation="3dp"
                android:gravity="top"
                android:letterSpacing="-0.03"
                android:paddingStart="@dimen/margin_basic1"
                android:paddingTop="@dimen/margin_basic1"
                android:text="@string/menu_title4"
                android:textSize="25dp"
                android:textStyle="bold"
                android:onClick="onClick"
                app:backgroundTint="@color/fundeagi_first2"
                app:rippleColor="#D6D6D6" />
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>